{
  "mcpServers": {
   "git-commit-mcp": {
      "disabled": false,
      "timeout": 60,
      "type": "stdio",
      "command": "npx",
      "args": [
        "git-commit-mcp"
      ],
      "env": {},
  "alwaysAllow": [
        "analyze_git_changes",
        "generate_commit_message",
        "stage_and_commit"
      ]
    }
  }
}
